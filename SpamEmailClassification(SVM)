    "    scores = cross_val_score(clf, X_train, Y_train, cv=3)\n",
    "    print(scores.mean())\n",
    "    clf.fit(X_train, Y_train)\n",
    "    Y_predTrain = clf.predict(X_train)\n",
    "    Y_predTest = clf.predict(X_test)\n",
